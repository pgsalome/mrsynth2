{
  "model": {
    "name": "diffusion",
    "diffusion": {
      "timesteps": 1000,
      "beta_schedule": "linear",
      "beta_start": 0.0001,
      "beta_end": 0.02,
      "sample_steps": 50,
      "use_snr_weighting": true
    },
    "unet": {
      "name": "unet_256",
      "num_downs": 8,
      "ngf": 64,
      "use_dropout": true
    }
  },
  "data": {
    "dataset_mode": "aligned",
    "batch_size": 4
  },
  "training": {
    "optimizer": {
      "name": "adam",
      "lr": 0.0001,
      "beta1": 0.9,
      "beta2": 0.999
    },
    "scheduler": {
      "name": "cosine",
      "params": {}
    }
  }
}