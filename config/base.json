{
  "data": {
    "dataset_dir": "./data/processed",
    "dataset_name": "dataset.pkl",
    "train_split": 0.7,
    "val_split": 0.15,
    "test_split": 0.15,
    "img_size": 256,
    "batch_size": 8,
    "num_workers": 4,
    "pin_memory": true,
    "shuffle": true,
    "direction": "AtoB",
    "input_nc": 3,
    "output_nc": 3,
    "no_flip": false,
    "cache_rate": 1.0,
    "proportion": 1.0,
    "test_mode": false
  },
  "training": {
    "num_epochs": 100,
    "early_stopping": {
      "patience": 10,
      "min_delta": 0.001
    },
    "optimizer": {
      "name": "adam",
      "lr": 0.0002,
      "beta1": 0.5,
      "beta2": 0.999,
      "weight_decay": 0.0001
    },
    "scheduler": {
      "name": "step",
      "params": {
        "step_size": 30,
        "gamma": 0.5
      }
    }
  },
  "logging": {
    "save_model_dir": "./saved_models",
    "log_dir": "./logs",
    "log_interval": 100,
    "image_log_freq": 10,
    "tensorboard": true,
    "wandb": {
      "enabled": true,
      "project": "mrsynth2",
      "entity": null,
      "name": null,
      "tags": [],
      "notes": ""
    }
  },
  "seed": 42,
  "gpu_ids": "0"
}